{
  "name": "@babili.io/angular",
  "version": "1.1.1",
  "description": "Types and services to create an Angular Babili client",
  "scripts": {
    "build": "rimraf dist && ng-packagr -p package.json",
    "deploy": "npm publish dist",
    "pack": "npm run build && cd dist && npm pack && cd .. && echo '\n\n\"@babili.io/angular\": \"file:<path>/dist/babili.io-angular-<your-version>.tgz\"\n\n'"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:Babili/babili-angular.git"
  },
  "peerDependencies": {
    "@angular/common": "^12.0.0",
    "@angular/core": "^12.0.0",
    "socket.io-client": "^4.1.3",
    "@types/socket.io-client": "^3.0.0",
    "moment": "^2.19.3",
    "rxjs": "^7.0.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "12.2.6",
    "@angular-devkit/build-ng-packagr": "0.1002.0",
    "@angular/common": "12.2.6",
    "@angular/compiler": "12.2.6",
    "@angular/compiler-cli": "12.2.6",
    "@angular/core": "12.2.6",
    "@types/socket.io-client": "3.0.0",
    "codelyzer": "6.0.2",
    "moment": "2.29.1",
    "ng-packagr": "12.2.0",
    "rxjs": "7.5.4",
    "socket.io-client": "4.4.1",
    "ts-node": "10.5.0",
    "tsickle": ">=0.46.0",
    "tslib": "2.3.1",
    "tslint": "6.1.3",
    "typescript": "4.3.5",
    "fsevents": "^2.1.2"
  },
  "ngPackage": {
    "$schema": "./node_modules/ng-packagr/ng-package.schema.json",
    "lib": {
      "entryFile": "src/index.ts",
      "umdModuleIds": {
        "moment": "moment",
        "socket.io-client": "socket.io-client"
      }
    },
    "dest": "dist"
  }
}
