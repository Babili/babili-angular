{
  "name": "@babili.io/angular",
  "version": "0.14.0",
  "description": "Types and services to create an Angular Babili client",
  "scripts": {
    "build": "rimraf dist && ng-packagr -p package.json",
    "deploy": "npm publish dist"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:Babili/babili-angular.git"
  },
  "peerDependencies": {
    "@angular/common": "^7.2.6",
    "@angular/core": "^7.2.6",
    "@types/socket.io-client": "1.4.32",
    "socket.io-client": "2.2.0",
    "moment": "^2.19.3",
    "rxjs": "^6.1.0"
  },
  "devDependencies": {
    "@types/socket.io-client": "1.4.32",
    "socket.io-client": "2.2.0",
    "@angular-devkit/build-angular": "~0.13.3",
    "@angular-devkit/build-ng-packagr": "~0.13.3",
    "@angular/common": "^7.2.6",
    "@angular/compiler": "^7.2.6",
    "@angular/compiler-cli": "^7.2.6",
    "@angular/core": "^7.2.6",
    "codelyzer": "~4.2.1",
    "moment": "^2.24.0",
    "ng-packagr": "^4.7.1",
    "rxjs": "^6.4.0",
    "ts-node": "~8.0.2",
    "tsickle": ">=0.25.5",
    "tslib": "^1.7.1",
    "tslint": "~5.13.0",
    "typescript": "~3.1.6"
  },
  "ngPackage": {
    "$schema": "./node_modules/ng-packagr/ng-package.schema.json",
    "lib": {
      "entryFile": "src/index.ts",
      "externals": {
        "moment": "moment"
      }
    },
    "dest": "dist"
  }
}
