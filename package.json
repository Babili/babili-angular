{
  "name": "@babili.io/angular",
  "version": "1.1.6",
  "description": "Types and services to create an Angular Babili client",
  "scripts": {
    "build": "rimraf dist && ng-packagr -p ng-package.json --config tsconfig.lib.json",
    "deploy": "cd dist && npm publish",
    "pack": "npm run build && cd dist && npm pack && cd .. && echo '\n\n\"@babili.io/angular\": \"file:<path>/dist/babili.io-angular-<your-version>.tgz\"\n\n'"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:Babili/babili-angular.git"
  },
  "peerDependencies": {
    "@angular/common": "^14.0.0",
    "@angular/core": "^14.0.0",
    "@types/socket.io-client": "^3.0.0",
    "rxjs": "^7.0.0",
    "socket.io-client": "^4.5.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "14.2.1",
    "@angular/common": "14.2.0",
    "@angular/compiler": "14.2.0",
    "@angular/compiler-cli": "14.2.0",
    "@angular/core": "14.2.0",
    "@types/estree": "^1.0.0",
    "@types/socket.io-client": "3.0.0",
    "codelyzer": "6.0.2",
    "fsevents": "^2.3.2",
    "dayjs": "^1.11.5",
    "ng-packagr": "14.2.0",
    "rxjs": "7.5.4",
    "socket.io-client": "4.5.1",
    "ts-node": "10.9.1",
    "tslib": "2.4.0",
    "tslint": "6.1.3",
    "typescript": "4.8.2"
  }
}
