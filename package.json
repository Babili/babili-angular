{
  "name": "@babili.io/angular",
  "version": "1.3.0",
  "description": "Types and services to create an Angular Babili client",
  "scripts": {
    "lint": "eslint .",
    "build": "rimraf dist && ng-packagr -p ng-package.json --config tsconfig.json",
    "deploy": "cd dist && npm publish",
    "pack": "npm run build && cd dist && npm pack && cd .. && echo '\n\n\"@babili.io/angular\": \"file:<path>/dist/babili.io-angular-'\"${npm_package_version}\"'.tgz\"\n\n'"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:Babili/babili-angular.git"
  },
  "peerDependencies": {
    "@angular/common": "^16.0.0",
    "@angular/core": "^16.0.0",
    "rxjs": "^7.0.0",
    "socket.io-client": "^4.5.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "16.0.0",
    "@angular-eslint/eslint-plugin": "16.0.1",
    "@angular-eslint/eslint-plugin-template": "16.0.1",
    "@angular/common": "16.0.0",
    "@angular/compiler": "16.0.0",
    "@angular/compiler-cli": "16.0.0",
    "@angular/core": "16.0.0",
    "@types/estree": "^1.0.0",
    "@types/socket.io-client": "3.0.0",
    "@typescript-eslint/eslint-plugin": "5.59.2",
    "@typescript-eslint/parser": "5.59.2",
    "codelyzer": "6.0.2",
    "dayjs": "^1.11.7",
    "eslint": "8.40.0",
    "fsevents": "^2.3.2",
    "ng-packagr": "16.0.0",
    "rxjs": "7.8.1",
    "socket.io-client": "4.6.1",
    "ts-node": "10.9.1",
    "tslib": "2.5.0",
    "tslint": "6.1.3",
    "typescript": "5.0.4"
  }
}
