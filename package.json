{
  "name": "@babili.io/angular",
  "version": "1.1.4",
  "description": "Types and services to create an Angular Babili client",
  "scripts": {
    "build": "rimraf dist && ng-packagr -p ng-package.json --config tsconfig.lib.json",
    "deploy": "cd dist && npm publish",
    "pack": "npm run build && cd dist && npm pack && cd .. && echo '\n\n\"@babili.io/angular\": \"file:<path>/dist/babili.io-angular-<your-version>.tgz\"\n\n'"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:Babili/babili-angular.git"
  },
  "peerDependencies": {
    "@angular/common": "^13.0.0",
    "@angular/core": "^13.0.0",
    "@types/socket.io-client": "^3.0.0",
    "rxjs": "^7.0.0",
    "socket.io-client": "^4.4.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "13.2.5",
    "@angular/common": "13.2.5",
    "@angular/compiler": "13.2.5",
    "@angular/compiler-cli": "13.2.5",
    "@angular/core": "13.2.5",
    "@types/estree": "^0.0.51",
    "@types/socket.io-client": "3.0.0",
    "codelyzer": "6.0.2",
    "fsevents": "^2.3.2",
    "dayjs": "^1.11.3",
    "ng-packagr": "13.2.1",
    "rxjs": "7.5.4",
    "socket.io-client": "4.4.1",
    "ts-node": "10.6.0",
    "tslib": "2.3.1",
    "tslint": "6.1.3",
    "typescript": "~4.4.4"
  }
}
