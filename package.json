{
  "name": "@babili.io/angular",
  "version": "17.0.0",
  "description": "Types and services to create an Angular Babili client",
  "scripts": {
    "lint": "eslint .",
    "build": "rimraf dist && ng-packagr -p ng-package.json --config tsconfig.json",
    "deploy": "cd dist && npm publish",
    "pack": "npm run build && cd dist && npm pack && cd .. && echo '\n\n\"@babili.io/angular\": \"file:<path>/dist/babili.io-angular-'\"${npm_package_version}\"'.tgz\"\n\n'"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:Babili/babili-angular.git"
  },
  "peerDependencies": {
    "@angular/common": "^17.0.0",
    "@angular/core": "^17.0.0",
    "rxjs": "^7.0.0",
    "socket.io-client": "^4.5.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "17.1.2",
    "@angular-eslint/eslint-plugin": "17.2.1",
    "@angular-eslint/eslint-plugin-template": "17.2.1",
    "@angular/common": "17.1.2",
    "@angular/compiler": "17.1.2",
    "@angular/compiler-cli": "17.1.2",
    "@angular/core": "17.1.2",
    "@types/estree": "^1.0.5",
    "@types/socket.io-client": "3.0.0",
    "@typescript-eslint/eslint-plugin": "6.21.0",
    "@typescript-eslint/parser": "6.21.0",
    "codelyzer": "6.0.2",
    "dayjs": "^1.11.10",
    "eslint": "8.56.0",
    "fsevents": "^2.3.3",
    "ng-packagr": "17.1.2",
    "rxjs": "7.8.1",
    "socket.io-client": "4.7.4",
    "ts-node": "10.9.2",
    "tslib": "2.6.2",
    "tslint": "6.1.3",
    "typescript": "5.3.3"
  }
}
