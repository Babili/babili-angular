{
  "name": "@babili.io/angular",
  "version": "18.0.1",
  "description": "Types and services to create an Angular Babili client",
  "scripts": {
    "lint": "eslint .",
    "build": "rimraf dist && ng-packagr -p ng-package.json --config tsconfig.json",
    "deploy": "cd dist && npm publish",
    "pack": "npm run build && cd dist && npm pack && cd .. && echo '\n\n\"@babili.io/angular\": \"file:<path>/dist/babili.io-angular-'\"${npm_package_version}\"'.tgz\"\n\n'"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:Babili/babili-angular.git"
  },
  "peerDependencies": {
    "@angular/common": "^18.0.0",
    "@angular/core": "^18.0.0",
    "rxjs": "^7.0.0",
    "socket.io-client": "^4.7.5"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "18.0.2",
    "@angular-eslint/eslint-plugin": "18.0.1",
    "@angular-eslint/eslint-plugin-template": "18.0.1",
    "@angular/common": "18.0.1",
    "@angular/compiler": "18.0.1",
    "@angular/compiler-cli": "18.0.1",
    "@angular/core": "18.0.1",
    "@types/estree": "^1.0.5",
    "@types/socket.io-client": "3.0.0",
    "@typescript-eslint/eslint-plugin": "7.11.0",
    "@typescript-eslint/parser": "7.11.0",
    "codelyzer": "6.0.2",
    "dayjs": "^1.11.11",
    "eslint": "8.57.0",
    "eslint-plugin-deprecation": "2.0.0",
    "eslint-plugin-unused-imports": "3.2.0",
    "fsevents": "^2.3.3",
    "ng-packagr": "18.0.0",
    "rxjs": "7.8.1",
    "socket.io-client": "4.7.5",
    "ts-node": "10.9.2",
    "tslib": "2.6.2",
    "tslint": "6.1.3",
    "typescript": "5.4.5"
  }
}
